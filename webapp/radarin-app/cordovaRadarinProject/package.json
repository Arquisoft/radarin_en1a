{
  "name": "cordovaRadarinProject",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "dependencies": {
    "@inrupt/solid-client": "^1.6.1",
    "@react-google-maps/api": "^2.1.1",
    "@solid/query-ldflex": "^2.11.3",
    "@solid/react": "^1.10.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "asciidoctor": "^2.2.1",
    "bootstrap": "^4.5.3",
    "cordova": "^10.0.0",
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0",
    "react-notifications-component": "^3.1.0",
    "react-scripts": "4.0.1",
    "solid-auth-client": "^2.5.5",
    "solid-file-client": "^1.2.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:e2e": "cd e2e && jest",
    "docs": "shx rm -rf build/docs && asciidoctor -D build/docs -a imagesdir=./images -r asciidoctor-diagram docs/index.adoc && shx cp -R docs/images build/docs",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-plugin-inappbrowser-popup-bridge": "^4.1.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "shx": "^0.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-inappbrowser-popup-bridge": {}
    },
    "platforms": [
      "android"
    ]
  },
  "homepage": "./"
}
